{
  "name": "hublab",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "clean": "rm -rf .next node_modules",
    "setup": "npm install && cp .env.example .env.local",
    "build:packages": "npm run build --workspaces",
    "build:ui": "npm run build --workspace=@hublab/ui",
    "build:sdk": "npm run build --workspace=@hublab/sdk",
    "dev:ui": "npm run dev --workspace=@hublab/ui",
    "dev:sdk": "npm run dev --workspace=@hublab/sdk",
    "export:capsules": "tsx scripts/export-capsules-to-json.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@sentry/nextjs": "^10.25.0",
    "@supabase/auth-helpers-nextjs": "^0.8.0",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/supabase-js": "^2.38.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "clsx": "^2.0.0",
    "csrf": "^3.1.0",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.16.0",
    "groq-sdk": "^0.34.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.33.0",
    "jose": "^6.1.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4",
    "resend": "^6.2.2",
    "stripe": "^13.0.0",
    "tailwindcss": "^3.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.29",
    "typescript": "^5.2.0"
  }
}
