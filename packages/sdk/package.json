{
  "name": "@hublab/sdk",
  "version": "1.0.1",
  "description": "Official TypeScript SDK for HubLab API - Build web apps with AI-powered components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hublab",
    "sdk",
    "api",
    "typescript",
    "web-builder",
    "react",
    "next.js",
    "ai-powered",
    "components",
    "no-code",
    "low-code"
  ],
  "author": "HubLab Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raym33/hublab.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://hublab.dev",
  "bugs": {
    "url": "https://github.com/raym33/hublab/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
