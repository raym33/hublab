{
  "name": "@hublab/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for HubLab API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm",
    "build:types": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hublab",
    "sdk",
    "api",
    "typescript",
    "web-builder",
    "no-code",
    "low-code"
  ],
  "author": "HubLab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hublab/sdk-typescript"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@hublab/sdk": "^1.0.0"
  }
}
